<ul>
  <% if @sla_ids.count == 1 %>
    <li><%= context_menu_link l(:button_show), sla_path(@sla), class: 'icon icon-magnifier', disabled: !@can[:show] %></li>
    <li><%= context_menu_link l(:button_edit), edit_sla_path(@sla), class: 'icon icon-edit', disabled: !@can[:edit] %></li>
  <% end %>
  <li><%= context_menu_link l(:button_delete),
            slas_path(id: @sla_ids, back_url: @back),
            method: :delete, class: 'icon icon-del', disabled: !@can[:delete],
            data: { confirm: l('sla_label.sla.count',count: @sla_ids.count)+"\n"+l(:text_are_you_sure) }
  %></li>
  <%= call_hook(:view_slas_context_menu_end, { sla: @sla, slas: @slas, can: @can, back: @back, project: @project }) %>
</ul>