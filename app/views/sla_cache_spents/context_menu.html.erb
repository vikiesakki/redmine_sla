<ul>
  <li><%= context_menu_link l(:button_refresh),
            refresh_sla_cache_spents_path(ids: @sla_cache_spent_ids, back_url: @back),
            class: 'icon icon-sla_cache_spent_refresh', disabled: !@can[:show]
  %></li>
  <li><%= context_menu_link l(:button_delete),
            sla_cache_spents_path(ids: @sla_cache_spent_ids, back_url: @back),
            method: :delete, class: 'icon icon-del', disabled: !@can[:delete],
            data: { confirm: l('sla_label.sla_cache_spent.count',count: @sla_cache_spent_ids.count)+"\n"+l(:text_are_you_sure) }
  %></li>
  <%= call_hook(:view_sla_cache_spents_context_menu_end, { sla_cache_spent: @sla_cache_spent, sla_cache_spents: @sla_cache_spents, can: @can, back: @back }) %>
</ul>